from sqlalchemy import create_engine
engine = create_engine('postgresql+psycopg2://postgres:5677@localhost/IBM_SQL')

#Check connection
engine.connect()

#Create table
metadata = MetaData()
books = Table('book', metadata,
  Column('id', Integer, primary_key=True),
  Column('title', String),
  Column('primary_author', String),
)
metadata.create_all(engine)

#Get column names
 
