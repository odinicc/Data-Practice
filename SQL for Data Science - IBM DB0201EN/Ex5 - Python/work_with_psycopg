#import lib
import psycopg2

#connect to db
conn = psycopg2.connect(
    host="localhost",
    database="IBM_SQL",
    user="postgres",
    password="5677")
    

#define cursor
cursor = conn.cursor()

#check database table names
cursor.execute("""SELECT table_name FROM information_schema.tables
       WHERE table_schema = 'public'""")
for table in cursor.fetchall():
    print(table)